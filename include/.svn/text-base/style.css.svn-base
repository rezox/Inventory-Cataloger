/* Default Facebook CSS */
.body
{
    font-family: "lucida grande" ,tahoma,verdana,arial,sans-serif;
    font-size: 11px;
    color: #333333;
}
/* Default Anchor Style */
.body a
{
    color: #3b5998;
    outline-style: none;
    text-decoration: none;
    font-size: 11px;
    font-weight: bold;
}
.body a:hover
{
    text-decoration: underline;
}

/* Facebook Tab Style */
.tab
{
    padding: 8px;
    font-size: 14px;
    background-color: #d8dfea;
    color: #3b5998;
    font-weight: bold;
    margin-right: 2px;
    text-decoration: none;
}
.tab:hover
{
    background-color: #3b5998;
    color: #ffffff;
    cursor: hand;
}